<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 连框颜色值 -->
    <item android:id="@+id/cycle_shape_bg">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/es_w"/>
            <stroke android:width="0.5dp" android:color="@color/colorPrimary"/>
            <size android:width="23dp" android:height="23dp"/>
        </shape>
    </item>

    <item
        android:id="@+id/cycle_shape_color"
        android:bottom="5dp"
        android:top="5dp"
        android:left="5dp"
        android:right="5dp">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/font_dark"/>
            <size android:width="20dp" android:height="20dp"/>
        </shape>
    </item>
</layer-list>